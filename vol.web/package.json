    "element-plus": "^2.9.4",
   "sortablejs": "^1.15.3",
    "less": "^4.4.0",
    "pinia": "^3.0.3",
    "qrcode.vue": "^3.6.0",
    "vue": "^3.5.13",
    "vue-draggable-next": "^2.2.1",
    "vue-router": "^4.5.1",
    "vuex": "^4.1.0",
    "wangeditor": "^4.7.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
